{
  "name": "saturn",
  "version": "0.1.1",
  "author": {
    "name": "Wesley Souza",
    "email": "wesleysoouza007@gmail.com"
  },
  "private": true,
  "scripts": {
    "cli": "node scripts/cli/cli.js",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "start:db": "docker start postgres || docker run --name postgres -e POSTGRES_PASSWORD=123321 -p 5432:5432 -d postgres",
    "stop:db": "docker stop postgres",
    "db:create": "docker exec meu_postgres createdb -U postgres meu_banco",
    "db:drop": "docker exec meu_postgres dropdb -U postgres meu_banco",
    "db:backup": "docker exec meu_postgres pg_dump -U postgres -F c meu_banco > backup.sql",
    "db:restore": "cat backup.sql | docker exec -i meu_postgres psql -U postgres meu_banco",
    "remove:db": "docker remove postgres",
    "docker:psql": "docker exec -it meu_postgres psql -U postgres",
    "docker:logs": "docker logs meu_postgres",
    "docker:restart": "docker restart meu_postgres",
    "docker:prune": "docker system prune -f",
    "docker:down": "docker-compose down",
    "docker:up": "docker-compose up -d",
    "logs:tail": "docker logs -f meu_postgres",
    "logs:clear": "docker logs --tail 0",
    "clean:cache": "next cache clear",
    "clean:node_modules": "rm -rf node_modules",
    "clean:build": "rm -rf .next",
    "clean:all": "pnpm run clean:cache && pnpm run clean:node_modules && pnpm run clean:build",
    "monitor:cpu": "docker stats --no-stream --format \"{{.Container}}: {{.CPUPerc}}\"",
    "monitor:mem": "docker stats --no-stream --format \"{{.Container}}: {{.MemUsage}}\"",
    "prisma:migrate": "pnpx prisma migrate dev",
    "prisma:generate": "pnpx prisma generate",
    "prisma:db:pull": "pnpx prisma db pull",
    "prisma:studio": "pnpx prisma studio",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:reset": "npx prisma migrate reset --force",
    "prisma:seed": "npx prisma db seed",
    "prisma:introspect": "npx prisma db introspect",
    "prisma:validate": "npx prisma validate",
    "prisma:clean": "npx prisma migrate reset --force && npx prisma migrate dev --name init",
    "env:copy": "cp .env.example .env",
    "env:sync": "sync-env",
    "setup": "pnpm install && pnpm run prisma:generate && pnpm run dev",
    "optimize:images": "next build && next export && imagemin public/* -o optimized/",
    "analyze:bundle": "next build && next export && next-bundle-analyzer",
    "analyze:webpack": "next build --profile && webpack-bundle-analyzer .next/stats.json",
    "deps:update": "pnpm update",
    "deps:check": "pnpm outdated",
    "deps:upgrade": "pnpx pnpm-check-updates -u && pnpm install",
    "deploy:staging": "next build && next export && vercel --pre",
    "deploy:preview": "vercel",
    "deploy": "pnpm run build && pnpm run prisma:deploy && pnpm run start",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod",
    "format": "prettier --write ."
  },
